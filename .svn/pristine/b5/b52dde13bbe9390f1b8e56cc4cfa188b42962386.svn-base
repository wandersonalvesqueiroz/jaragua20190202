html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, embed,
figure, figcaption, footer, header, hgroup,
menu, nav, output, ruby, section, summary,
time, mark, audio, video {
    margin: 0;
    padding: 0;
    border: 0;
    font-size: 100%;
    font: inherit;
    vertical-align: baseline;
}

/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure,
footer, header, hgroup, menu, nav, section {
    display: block;
}

body {
    line-height: 1;
}

ol, ul {
    list-style: none;
}

blockquote, q {
    quotes: none;
}

blockquote:before, blockquote:after,
q:before, q:after {
    content: '';
    content: none;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}

/**  General **/

* {
    margin: 0;
}

html {
    overflow-y: scroll;
}

html, body {
    height: 100%;
}

body {
    text-align: left;
    font-family: 'Libre Franklin', Helvetica, Arial;
    font-weight: 200;
    font-size: 16px;
    line-height: 1.4;
    color: #504f51;
}

a {
    color: #504f51;
    text-decoration: underline;
    transition: all 300ms;
}

a:hover {
    color: #000000;
    text-decoration: underline;
}

img {
    border-style: none;
}

em, i {
    font-style: italic;
}

strong, b {
    font-weight: bold;
}

p {
    line-height: 1.4;
    margin: 8px 0;
}

h1 {
    font-weight: 400;
    font-size: 22px;
    line-height: 1.15em;
    margin-bottom: 12px;
}

h2 {
    font-weight: 400;
    color: #504f51;
    font-size: 18px;
    line-height: 1.15em;
    margin-bottom: 10px;
}

h3 {
    font-weight: 400;
    color: #504f51;
    font-size: 18px;
    line-height: 1.15em;
    margin-bottom: 6px;
}

h4 {
    font-weight: 600;
    color: #000000;
    font-size: 14px;
}

h5 {
    font-weight: 300;
    color: #000000;
    font-size: 14px;
}

h6 {
    font-weight: 600;
    font-size: 13px;
}

ul, ol {
    list-style-position: outside;
    list-style-type: circle;
    padding-left: 30px;
    margin-top: 6px;
    margin-bottom: 6px;
}

hr {
    border: 0;
    color: rgb(226, 226, 226);
    background-color: rgb(226, 226, 226);
    height: 1px;
    margin-top: 5px;
    margin-bottom: 5px;
}

button {
    cursor: pointer;
    transition: all 300ms;
}

.tip {
    font-size: 0.9em;
    text-align: left;
    line-height: 1.2em;
    padding: 3px;
    max-width: 400px
}

.tip-title {
    font-weight: bold;
}

#system-message {
    padding: 0;
    margin: 0;
}

#wrapper {
    display: block;
    padding: 0;
    min-height: 100%;
    height: 100%;
    margin: 0 auto;
}

/** Buttoms **/

#login-form input.button,
#member-profile button.validate,
#member-registration button.validate,
#user-registration button.validate,
.logout button,
.login button,
#searchForm button,
#contact-form button.validate,
#mailto-window button,
.readmore a,
.btn {
    font-weight: 200;
    font-size: 14px;
    color: #fff;
    background-color: #504f51;
    border: 0;
    padding: 5px 20px;
    margin: 0;
    text-decoration: none;
    display: table;
}

#login-form input.button:hover,
#member-profile button.validate:hover,
#member-registration button.validate:hover,
#user-registration button.validate:hover,
.logout button:hover,
.login button:hover,
#searchForm button:hover,
#contact-form button.validate:hover,
#mailto-window button:hover,
.readmore a:hover,
.btn:hover {
    color: #fff;
    text-decoration: none;
    background-color: #000000;
}

.readmore a {
    float: right;
    font-style: italic;
}

/** Header **/

#header_wrap {
    width: 100%;
    display: table;
}

#palmeiras {
    width: 100%;
    display: table;
    position: relative;
}

#palmeiras img {
    position: absolute;
    left: 0;
    top: 0;
    display: table;
}

#palmeiras img:last-child {
    right: 0;
    left: auto;
}

#header {
    width: 1170px;
    display: table;
    margin: auto;
}

#logo {
    float: left;
}

#logo a {
    display: table;
    padding: 10px 0 5px 0;
    line-height: 0;
}

#topnav_wrap {
    width: 100%;
    display: table;
}

#search {
    clear: right;
    float: right;
    margin-top: 25px;
    white-space: nowrap;
}

#search > div {
    display: inline-block;
    vertical-align: top;
}

.search-searchword {
    margin-right: 2px;
}

.btn-icon-search {
    padding: 0px;
    width: 30px;
    line-height: 30px;
    border-radius: 0 5px 5px 0;
    font-size: 18px;
    margin-left: -4px;
    display: inline-block;
}

input.search-query {
    border-radius: 5px 0 0 5px;
    border: 0;
    background-color: #e2e4e7;
    font-family: 'Libre Franklin';
    font-weight: 400;
    padding: 0 15px;
    line-height: 30px;
    vertical-align: top;
    display: inline-block;
}

.btn-green {
    background-color: #41583e;
}

.btn-green:hover {
    background-color: #183d13;
}

.element-invisible {
    display: none;
}

#login,
#logout {
    position: relative;
}

.btn-login-logout {
    background-color: #41583e;
    color: #FFF;
    margin-left: 5px;
    line-height: 30px;
    font-size: 14px;
    padding: 0 10px;
    border-radius: 5px;
    vertical-align: middle;
    cursor: pointer;
}

.btn-login-logout:hover {
    background-color: #183d13;
}

.btn-login-logout span {
    margin-left: 5px;
}

#login-form {
    margin-top: 5px;
    display: none;
    position: absolute;
    right: 0;
    padding: 10px 20px;
    background-color: #41583e;
    color: #FFF;
    z-index: 999;
}

.login-button {
    display: table;
    margin: 10px auto;
    background-color: #FFF;
    border-radius: 5px;
    color: #183d13;
    text-transform: uppercase;
}

.login-button:hover {
    color: #FFF;
    background-color: #183d13;
}

/** Topmenu (added multi-level support v1.1) **/

#topmenu_wrap {
    width: 100%;
    display: table;
}

#topmenu {
    width: 1170px;
    display: table;
    margin: auto;
}

#topmenu ul {
    display: flex;
    flex-flow: row wrap;
    justify-content: space-between;
    list-style: none;
    margin: 0;
    padding: 0;
}

#topmenu li {
    flex: 1 1 auto;
    position: relative;
    text-align: center;
}

#topmenu span {
    cursor: default;
}

#topmenu span,
#topmenu a {
    font-size: 14px;
    font-weight: 400;
    width: 100%;
    display: table;
    padding: 8px 0;
    text-decoration: none;
    color: #004f76;
    text-transform: uppercase;
}

#topmenu span:hover,
#topmenu li.active span,
#topmenu li.active span:hover,
#topmenu a:hover,
#topmenu li.active a,
#topmenu li.active a:hover {
    color: #257ba5;
}

/** Second-level lists **/

#topmenu li ul {
    position: absolute;
    left: -999em;
    border: 0;
    z-index: 100;
}

#topmenu li li,
#topmenu li li:first-child {
    border: 0;
    flex: auto;
    width: 100%;
    background-color: #FFF;
}

#topmenu li li a {
    border: 0;
    padding: 8px 0;
    font-size: 12px;
    width: 100%;
}

#topmenu li li.active a,
#topmenu li li.active a:hover {
    background-color: #0a3d57;
    color: #e6e6e7;
}

#topmenu li.active ul a:hover {
    text-decoration: none;
}

#topmenu li.active ul a {
    text-decoration: none;
}

/** Level third and above **/

#topmenu li ul ul {
    margin: -39px 0 0 220px;
}

#topmenu li:hover ul ul, #topmenu li:hover ul ul ul, #topmenu li.sfhover ul ul, #topmenu li.sfhover ul ul ul {
    left: -999em;
}

#topmenu li:hover ul, #topmenu li li:hover ul, #topmenu li li li:hover ul, #topmenu li.sfhover ul, #topmenu li li.sfhover ul, #topmenu li li li.sfhover ul { /* nested under hovered items */
    left: auto;
}

/** Content/Menu Wrap **/

#content-menu_wrap_bg {
    width: 100%;
    display: table;
}

#content-menu_wrap {
    width: 1170px;
    display: table;
    margin: auto;
}

#content-menu_wrap img {
    max-width: 100%;
}

/** Breadcrumbs **/

#breadcrumbs {
    width: auto;
    font-size: 0.89em;
    text-decoration: none;
    margin-bottom: 0px;
    margin-top: 16px;
}

/** Menus (Left/Right) **/

#leftmenu {
    width: calc(25% - 30px);
    margin: 60px 30px 60px 0;
    padding: 0px;
    float: left;
}

#rightmenu {
    width: calc(25% - 30px);
    margin: 60px 0 60px 30px;
    padding: 0px;
    float: right;
}

#leftmenu ul.menu a,
#rightmenu ul.menu a {
    color: #0a3d57;
    padding: 8px 20px;
    text-decoration: none;
    width: calc(100% - 40px);
    display: table;
}

#leftmenu ul.menu a:hover,
#rightmenu ul.menu a:hover {
    color: #e6e6e7;
    background-color: #0a3d57;
    text-decoration: none;
}

#leftmenu ul.menu,
#rightmenu ul.menu {
    list-style-type: none;
    padding: 10px 0;
    background-color: #e6e6e7;
    margin: 0;
}

#leftmenu ul.menu li,
#rightmenu ul.menu li {
    border-bottom: 1px solid #0a3d57;
    list-style-type: none;
}

#leftmenu ul.menu li:last-child,
#rightmenu ul.menu li:last-child {
    border: 0;
}

#leftmenu ul.menu li ul,
#rightmenu ul.menu li ul {
    margin-top: 0px;
    margin-bottom: 0px;
}

#leftmenu ul.menu li ul li,
#rightmenu ul.menu li ul li {
    border: 0px;
    padding-bottom: 2px;
    list-style-type: none;
    font-size: 0.96em;
    line-height: 1.5em;
}

#leftmenu ul.menu li.current a,
#rightmenu ul.menu li.current a {
    color: #e6e6e7;
    background-color: #0a3d57;
}

#leftmenu ul.menu li.current ul li a,
#rightmenu ul.menu li.current ul li a {
    font-weight: normal;
}

.moduletable_menu {
    clear: both;
}

#topnav div.moduletable {
    clear: both;
    overflow: hidden;
    margin-bottom: 0px;
}

.moduletable_menu h3 {
    font-family: 'Droid Sans', sans-serif;
    font-weight: 400;
    color: #000000;
    font-size: 18px;
    margin-bottom: 6px;
}

/** Content **/

#content-w1 { /** left and right menu visible **/
    width: 50%;
    float: left;
    padding-top: 60px;
    overflow: hidden;
    margin-bottom: 60px;
}

#content-w2 { /** left or right menu visible **/
    width: 75%;
    float: left;
    padding-top: 60px;
    overflow: hidden;
    margin-bottom: 60px;
}

#content-w3 { /** no menu visible **/
    width: 100%;
    float: left;
    padding-top: 60px;
    overflow: hidden;
    margin-bottom: 60px;
}

#content-top {
    display: table;
    width: 100%;
}

.moduletable-full-logos {
    width: 1170px;
    display: table;
    margin: 60px auto;
}

ul.actions {
    margin: 0;
    padding: 0;
    margin-top: -6px;
    margin-bottom: -2px;
    list-style-type: none;
    text-align: right;
}

